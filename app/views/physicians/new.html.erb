<h1>Physicians#new</h1>
<h1>Add Physician</h1>
<%= form_with(model: @physician, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "Physician Name" %>
  </div>
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>